# LiveKit
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret
LIVEKIT_ROOM=deepresearchpod-duo

# Optional: pre-generated tokens (skip token generation if set)
LIVEKIT_TOKEN_PETER=
LIVEKIT_TOKEN_STEWIE=

# Gemini
GEMINI_API_KEY=your-gemini-api-key
GEMINI_MODEL=gemini-2.5-flash

# ElevenLabs
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_MODEL_ID=eleven_multilingual_v2
ELEVENLABS_OUTPUT_FORMAT=pcm_16000
ELEVENLABS_VOICE_ID_PETER=voice_id_for_peter
ELEVENLABS_VOICE_ID_STEWIE=voice_id_for_stewie

# Agent behavior
AGENT_INPUT_MODE=stdin  # stdin | livekit
MAX_HISTORY_TURNS=6
